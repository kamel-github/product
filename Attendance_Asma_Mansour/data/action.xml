<?xml version="1.0" ?>
<odoo>
	<data noupdate="1">

		<record id="server_action_force_sync" model="ir.actions.server">
			<field name="name">Force Sync attendances</field>
			<field name="type">ir.actions.server</field>
			<field name="model_id" ref="model_hr_attendance"/>
			<field name="state">code</field>
			<field name="code">
				if records:
				action = records.action_attendance_sync()
			</field>
			<field name="binding_model_id" ref="model_hr_attendance"/>
		</record>
	</data>
</odoo>