<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="hr_contract_history_view_form" model="ir.ui.view">
        <field name="name">hr.contract.history.view</field>
        <field name="model">hr.contract.history</field>
        <field name="inherit_id" ref="hr_contract.hr_contract_history_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='contract_history']/field[@name='contract_ids']/tree/field[@name='wage']" position="after">
                <field name="net"/>
                <field name="contract_type_id"/>
            </xpath>
        </field>
    </record>
</odoo>